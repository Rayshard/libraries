{
    "terminals": {
        "default": {
            "(_|[a-zA-z])(_|[a-zA-z]|[0-9])*": {
                "name": "ID",
                "return": "Token(TokenType::ID, @value, Position(@line, @column)",
                "action": "print(f\"({}, {}) {}\", @value, @line, @column)"
            },
            "-?(0|[1-9][0-9]*)": {
                "name": "Number",
                "return": "Token(TokenType::NUMBER, @value, @position)",
                "action": "print(f\"({}, {}) {}\", @value, @line, @column)"
            },
            "\"": {
                "name": ""
            }
        },
        "string_literal": {}
    },
    "nonterminals": {
        "DECL": {
            "rules": [
                {"name": "full", "production": ["let", "ID", "=", "EXPR", ";"]},
                {"name": "no_expr", "production": ["let", "ID", ";"]}
            ],
            "action": "print(\"Found variable declaration!\")"
        }
    }
}